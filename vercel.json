{
	"version": 2,
	"builds": [
		{
			"src": "doctor-consultation-backend/api/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "^/api(/.*)?$",
			"dest": "/doctor-consultation-backend/api/index.js"
		},
		{ "src": "^/$", "dest": "/doctor-consultation-backend/api/index.js" }
	]
}

